{
    "name": "leboncampus",
    "description": "Web site for Le Bon Campus association",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "silex/silex": "~2.0",
        "silex/web-profiler": "~2.0",
        "symfony/asset": "3.4.6",
        "symfony/browser-kit": "3.4.6",
        "symfony/class-loader": "3.4.6",
        "symfony/config": "3.4.6",
        "symfony/console": "3.4.6",
        "symfony/css-selector": "3.4.6",
        "symfony/dom-crawler": "3.4.6",
        "symfony/filesystem": "3.4.6",
        "symfony/inflector": "3.4.6",
        "symfony/property-access": "3.4.6",
        "symfony/options-resolver": "3.4.6",
        "symfony/var-dumper": "3.4.6",
        "symfony/intl": "3.4.6",
        "symfony/debug": "3.4.6",
        "symfony/finder": "3.4.6",
        "symfony/form": "3.4.6",
        "symfony/translation": "3.4.6",
        "symfony/process": "3.4.6",
        "symfony/security": "3.4.6",
        "symfony/twig-bridge": "3.4.6",
        "symfony/validator": "3.4.6",
        "doctrine/dbal": "2.5.13"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web"
        ]
    }
}
